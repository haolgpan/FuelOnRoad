            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/msg_odeusz_piotrows"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtTue"
            tools:ignore="TextSizeCheck"
            tools:text="@string/msg_odeusz_piotrows" />

        <TextView
            android:id="@+id/txtMaciejOrowski"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_maciej_or_owski"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtMarCounter"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_maciej_or_owski" />

        <ImageView
            android:id="@+id/imageImageThree"
            android:layout_width="@dimen/_46pxh"
            android:layout_height="@dimen/_44pxh"
            android:layout_gravity="center_horizontal"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtBoenkaMalina"
            tools:ignore="ContentDescription"
