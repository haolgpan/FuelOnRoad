            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/msg_maciej_kowalski"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageImage"
            tools:ignore="TextSizeCheck"
            tools:text="@string/msg_maciej_kowalski" />

        <TextView
            android:id="@+id/txtBoenkaMalina"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_bo_enka_malina"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtSun"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_bo_enka_malina" />

        <TextView
            android:id="@+id/txtSun"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_sun"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtWilldosuper"
            tools:ignore="TextSizeCheck,TextSizeCheck"
            tools:text="@string/lbl_sun" />

        <TextView
            android:id="@+id/txtTue"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_tue"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtEmail"
            tools:ignore="TextSizeCheck,TextSizeCheck"
            tools:text="@string/lbl_tue" />

        <ImageView
            android:id="@+id/imageImage"
            android:layout_width="@dimen/_46pxh"
            android:layout_height="@dimen/_44pxh"
            android:layout_gravity="center_horizontal"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtMaciejKowalski"
            tools:ignore="ContentDescription"
            app:srcCompat="@drawable/img_image" />

        <TextView
            android:id="@+id/txtMarCounter"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_23_mar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtUploadedfile"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_23_mar" />

        <TextView
            android:id="@+id/txtUploadedfile"
            style="@style/txtRobotolight13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_uploaded_file"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageImageThree"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_uploaded_file" />

        <ImageView
            android:id="@+id/imageGroup"
            android:layout_width="@dimen/_15pxh"
            android:layout_height="@dimen/_15pxh"
            android:layout_gravity="center_horizontal"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageFavorite"
            tools:ignore="ContentDescription"
            app:srcCompat="@drawable/img_group" />

        <TextView
            android:id="@+id/txtHereisanother"
            style="@style/txtRobotolight13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/msg_here_is_another"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageImageFour"
            tools:ignore="TextSizeCheck"
            tools:text="@string/msg_here_is_another" />

        <TextView
            android:id="@+id/txtKrysiaEurydyka"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_krysia_eurydyka"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtMarCounterOne"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_krysia_eurydyka" />

        <TextView
            android:id="@+id/txtMaciejKowalski"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="1dp"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_maciej_kowalski"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtTime"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_maciej_kowalski" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearRowndiceone"
            style="@style/groupStylelime_600_7f"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal"
            android:layout_marginTop="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtKrysiaEurydyka"
            app:layout_constraintVertical_bias="1.0">

            <ImageView
                android:id="@+id/imageMap"
                android:layout_width="@dimen/_43pxh"
                android:layout_height="34dp"
                android:layout_marginHorizontal="15dp"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageGasdarkblueOne"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageSettings"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription"
                app:srcCompat="@drawable/img_map" />

            <ImageView
                android:id="@+id/imageGasdarkblueOne"
                android:layout_width="@dimen/_28pxh"
                android:layout_height="0dp"
                android:layout_marginHorizontal="15dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_gasdarkblue2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/viewRectangle925"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageMap"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_gasdarkblue2" />

            <ImageView
                android:id="@+id/imageSettings"
                android:layout_width="@dimen/_41pxh"
                android:layout_height="32dp"
                android:layout_marginHorizontal="15dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_ndice2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageMap"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageWarning"
                android:layout_width="@dimen/_45pxh"
                android:layout_height="34dp"
                android:layout_marginHorizontal="15dp"
                android:scaleType="fitXY"
                android:src="@drawable/img_warning"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/viewRectangle925"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_warning" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/viewRectangle925"
                android:layout_width="50dp"
                android:layout_height="0dp"
                android:layout_gravity="start"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp"
                android:background="@drawable/rectangle_bg_bluegray_900_radius_15"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageWarning"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/imageGasdarkblueOne"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/imageNdiceOne"
                    android:layout_width="31dp"
                    android:layout_height="30dp"
                    android:layout_marginHorizontal="15dp"
                    android:scaleType="fitXY"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.526"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.2"
                    tools:ignore="ContentDescription"
                    app:srcCompat="@drawable/img_settings" />
