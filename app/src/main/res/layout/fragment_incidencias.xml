            app:layout_constraintTop_toTopOf="@+id/txtSearch"
            app:srcCompat="@drawable/img_subtract"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/txtChatrooms"
            style="@style/txtRobotoregular20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginTop="16dp"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_chatrooms"
            app:layout_constraintStart_toStartOf="@+id/txtSearch"
            app:layout_constraintTop_toBottomOf="@+id/txtSearch"
            tools:text="@string/lbl_chatrooms" />

        <TextView
            android:id="@+id/txtTime"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="28dp"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_08_43"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtChatrooms"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_08_43" />

        <ImageView
            android:id="@+id/imageImageOne"
            android:layout_width="@dimen/_46pxh"
            android:layout_height="@dimen/_44pxh"
            android:layout_gravity="center_horizontal"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtOdeuszPiotrows"
            tools:ignore="ContentDescription"
            app:srcCompat="@drawable/img_image_44x46" />

        <TextView
            android:id="@+id/txtMarCounterOne"
            style="@style/txtRobotoregular15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:ellipsize="end"
            android:letterSpacing="0.05"
            android:singleLine="true"
            android:text="@string/lbl_18_mar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtHereisanother"
            tools:ignore="TextSizeCheck"
            tools:text="@string/lbl_18_mar" />

        <TextView
            android:id="@+id/txtOdeuszPiotrows"
